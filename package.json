{
  "name": "caesar-electron-vite-antd",
  "private": true,
  "version": "0.1.0",
  "main": "dist/main/index.js",
  "author": {
    "email": "454451758@qq.com",
    "name": "Magento Electron"
  },
  "workspaces": [
    "workspaces/*"
  ],
  "scripts": {
    "dev": "concurrently \"yarn dev:vite\" \"yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "yarn build:electron && electron .",
    "build": "yarn build:vite && yarn build:electron",
    "build:vite": "tsc && vite build",
    "build:electron": "tsc -p electron",
    "clean": "rm -rf ./build && rm -rf ./release",
    "release": "yarn build && electron-builder --dir",
    "release:win": "electron-builder --win",
    "release:mac": "electron-builder --mac",
    "release:linux": "electron-builder --linux"
  },
  "dependencies": {
    "@apollo/client": "^3.7.2",
    "electron-is-dev": "^2.0.0",
    "nookies": "^2.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.5"
  },
  "devDependencies": {
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^2.2.0",
    "concurrently": "^7.6.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
